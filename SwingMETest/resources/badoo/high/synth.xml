    <style id="checkbox">
        <imageIcon id="check" path="/images/iconGrid.png" x="0" y="23" width="23" height="23"/>
        <imageIcon id="check_selected" path="/images/iconGrid.png" x="0" y="0" width="23" height="23"/>
        <imageIcon id="check_focused" path="/images/iconGrid.png" x="0" y="69" width="23" height="23"/>
